my_prompt() {
    last_return_code=$?;
    last_return_code_out="("$last_return_code")"
    if [ $last_return_code -ne 0 ]; then
        last_return_code_out="\033[0;31m("$last_return_code")";
    fi
    ## output return code
    echo -ne "\033[1;37m$0:[return code: $last_return_code_out\033[1;37m]\n\n"$NO_COLOR
    (git_prompt_autofetch &)
    gitPrompt=$(git_prompt)
    echo -ne "\033[1;37m$0:$NO_COLOR$gitPrompt\033[1;34m$PWD\033[0m\n"
}
